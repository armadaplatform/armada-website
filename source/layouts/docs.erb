<% wrap_layout :layout do %>
<div class="docheader navbar navbar-fixed-top">
  <header id="header">
    <%= partial 'layouts/_global_nav' %>
  </header>
</div>
  <div class="row body-content-container wrap-container" role="main">
    <nav id="toc" class="sidebar col-md-offset-1 col-md-2 col-sm-3 col-xs-12" data-waypoint="sidebar">
      <%= partial 'layouts/_documentation_nav' %>
    </nav>
    <main class="main col-sm-9 col-md-8 col-xs-12" role="main" data-waypoint="main">
      <%= yield %>
    </main>
  </div>
<% end %>
